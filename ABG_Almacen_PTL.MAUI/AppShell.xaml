<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ABG_Almacen_PTL.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ABG_Almacen_PTL.MAUI"
    xmlns:pages="clr-namespace:ABG_Almacen_PTL.MAUI.Pages"
    Title="ABG Almacén PTL">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate pages:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Ubicar BAC"
        ContentTemplate="{DataTemplate pages:UbicarBACPage}"
        Route="UbicarBACPage" />

    <ShellContent
        Title="Extraer BAC"
        ContentTemplate="{DataTemplate pages:ExtraerBACPage}"
        Route="ExtraerBACPage" />

    <ShellContent
        Title="Empaquetar BAC"
        ContentTemplate="{DataTemplate pages:EmpaquetarBACPage}"
        Route="EmpaquetarBACPage" />

    <ShellContent
        Title="Consulta PTL"
        ContentTemplate="{DataTemplate pages:ConsultaPTLPage}"
        Route="ConsultaPTLPage" />

    <ShellContent
        Title="Repartir Artículo"
        ContentTemplate="{DataTemplate pages:RepartirArticuloPage}"
        Route="RepartirArticuloPage" />

</Shell>
